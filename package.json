{
  "name": "ui-toolkit",
  "version": "0.0.0",
  "description": "React & tailwind design system",
  "main": "src/index.ts",
  "scripts": {
    "test": "tsc --project tsconfig.json",
    "clean": "rm -rf dist",
    "build": "yarn clean && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pre-commit": "pretty-quick --staged",
    "prettier": "prettier --write .",
    "prepare": "yarn husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexander-mann/ui-toolkit.git"
  },
  "keywords": [
    "design",
    "system",
    "react",
    "tailwind",
    "typescript",
    "ui",
    "toolkit",
    "components",
    "utilities",
    "theme"
  ],
  "author": "Alexander Mann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexander-mann/ui-toolkit/issues"
  },
  "homepage": "https://github.com/alexander-mann/ui-toolkit#readme",
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/js": "^9.20.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/addon-onboarding": "^8.5.3",
    "@storybook/addon-styling-webpack": "^1.0.1",
    "@storybook/addon-themes": "^8.5.3",
    "@storybook/addon-webpack5-compiler-swc": "^2.0.0",
    "@storybook/manager-api": "^8.5.3",
    "@storybook/react-webpack5": "^8.5.3",
    "eslint": "^9.20.0",
    "husky": "^9.1.7",
    "plop": "^4.0.1",
    "postcss": "^8.5.1",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "pretty-quick": "^4.0.0",
    "storybook": "^8.5.3",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind": "^4.0.0",
    "tailwind-merge": "^3.0.1"
  }
}
