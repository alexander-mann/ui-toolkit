{
  "name": "@alexandermann/ui-toolkit",
  "version": "0.0.4",
  "description": "React & tailwind design system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "tsc --project tsconfig.json",
    "clean": "rm -rf dist",
    "build": "yarn clean && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "generate:component": "plop component",
    "storybook": "BROWSER=none storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pre-commit": "pretty-quick --staged",
    "prettier": "prettier --write .",
    "npm-publish": "npm run build && npm publish --access public",
    "prepare": "yarn husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexander-mann/ui-toolkit.git"
  },
  "keywords": [
    "design",
    "system",
    "react",
    "tailwind",
    "typescript",
    "ui",
    "toolkit",
    "components",
    "utilities",
    "theme"
  ],
  "author": "Alexander Mann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexander-mann/ui-toolkit/issues"
  },
  "homepage": "https://github.com/alexander-mann/ui-toolkit#readme",
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@storybook/addon-a11y": "^8.5.3",
    "@storybook/addon-essentials": "^8.3.4",
    "@storybook/addon-interactions": "^8.3.4",
    "@storybook/addon-links": "^8.3.4",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/addon-themes": "^8.3.4",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.5",
    "@storybook/blocks": "^8.3.4",
    "@storybook/manager-api": "^8.3.5",
    "@storybook/react": "^8.3.4",
    "@storybook/react-webpack5": "^8.3.4",
    "@storybook/test": "^8.3.4",
    "@storybook/theming": "^8.3.5",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "css-loader": "^7.1.2",
    "eslint": "^9.20.0",
    "husky": "^9.1.7",
    "plop": "^4.0.1",
    "postcss": "^8.5.1",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "pretty-quick": "^4.0.0",
    "storybook": "^8.5.3",
    "style-loader": "^3.3.4",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript-eslint": "^8.23.0",
    "webpack": "^5.90.3"
  },
  "dependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/postcss": "^4.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.475.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind": "^4.0.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^3.4.13",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.3"
  }
}
